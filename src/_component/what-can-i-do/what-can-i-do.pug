-
    var header = '#what I can do';
    var whatCanIDoList = [
        {
            title: 'Font End',
            skillList: [
                'HTML5',
                'CSS3',
                'React',
                'Cal',
                'Capra',
                'oaie'
            ]
        },
        {
            title: 'Back End',
            skillList: [
                'FireBase',
                'Git',
                'Caine',
                'Pisica',
                'Sarmale',
                'Paine',
                'Smantana'
            ]
        }
    ]

#what-can-i-do
.what-can-i-do
    .what-can-i-do__header=header
    each whatCanIDo in whatCanIDoList
        .what-can-i-do__title=whatCanIDo.title
        .what-can-i-do__list
            each skill in whatCanIDo.skillList
                .what-can-i-do__skill=skill